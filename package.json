{
  "name": "framed-moments-mcp",
  "version": "0.1.0",
  "description": "MCP server for capturing and framing moments of experience",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "framed-moments-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc-watch --onSuccess \"node dist/index.js\"",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "rebuild-embeddings": "ts-node scripts/rebuild-embeddings.ts"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "experience",
    "moments",
    "capture",
    "phenomenology",
    "experiential",
    "qualities",
    "embodied",
    "lived-experience",
    "attention",
    "awareness",
    "sensory",
    "reflection"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "latest",
    "@xenova/transformers": "^2.17.2",
    "chrono-node": "^2.6.0",
    "dotenv": "^17.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.3.3"
  }
}
